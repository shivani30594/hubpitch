<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../adminComponents/headerFile.ejs %>
</head>

<body>
    <% include ../adminComponents/header.ejs %>
    <div class="form-wrapper">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="card-box">

                        <h4 class="m-t-0 header-title">All Subscription</h4>
                        <p class="text-muted m-b-30 font-14">Manage All Subscription Plans From Here.</p>

                        <div class="row data-table-wapper">
                            <table id="example" class="table table-striped table-bordered" style="width:100%">
                                <thead>
                                    <tr>
                                        <th>Plan Name</th>
                                        <th>Plan Price</th>
                                        <th>Created</th>
                                        <th>Actions</th>
                                    </tr>
                                </thead>
                                <tbody id="tbody_datatable">
                                    <% for(var i=0; i < data.length; i++) { %>
                                    <tr>
                                        <td>
                                            <%= data[i].plan_name %>
                                        </td>
                                        <td>
                                            <%= data[i].plan_price %>
                                        </td>
                                        <td>
                                            <%= data[i].niceDate %>
                                        </td>
                                        <td>
                                            <a class="action-table" href="/admin/edit-plan/<%= data[i].plan_id %>">
                                                <span class="glyphicon glyphicon-pencil"></span>
                                            </a>
                                            <a class="action-table" href="javascript:void(0)">
                                                <span class="glyphicon glyphicon-trash"></span>
                                            </a>
                                        </td>
                                    </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Modal -->
    <!-- Modal -->
    <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="myModalLabel">Modal title</h4>
                </div>
                <div class="modal-body">

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div>
    </div>
    <!-- LOADER  -->
    <div class="loader_hp_">
        <i class="fa fa-spinner fa-spin" style="font-size:24px"></i>
    </div>

    <script src="/js/module/admin/subscriptionModule/manageMembership.js"></script>
</body>

</html>